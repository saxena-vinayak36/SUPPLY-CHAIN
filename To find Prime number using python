r=int(input("Enter a integer:"))
if r>1:
    for i in range(2,r):
       if r%i==0:
         print(str(r)+"is not a prime number")
         print(str(i)+" time "+str(r//i)+" is "+str(r))
         break
       else:
        print(str(r)+" is a prime number.")
    
else:
    print(str(r)+" is negative integer")
print("Now enjoy Hacktoberfest! :D ")
